# Example environment configuration for containers_fetcher
# Copy this file to `.env` and update the values for your environment.

# HTTP
PORT=port

# API authentication
API_KEY_HEADER=key
API_KEYS=key-1,key-2

# Firebird database connection
FIREBIRD_HOST=host
FIREBIRD_PORT=port
FIREBIRD_DATABASE=path
FIREBIRD_USER=user
FIREBIRD_PASSWORD=password
FIREBIRD_ROLE=role
FIREBIRD_PAGE_SIZE=size
FIREBIRD_CHARSET=UTF8
FIREBIRD_WIRE_CRYPT=crypt
FIREBIRD_AUTH_PLUGINS=plugins
FIREBIRD_PLUGIN_NAME=
FIREBIRD_LIBRARY_PATH=
BCT_LOOKUP_CONCURRENCY=5
